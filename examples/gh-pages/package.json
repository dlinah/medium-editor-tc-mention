{
  "name": "gh-pages",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ../../public/index.html ../../public/assets",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production reacthtmlpack buildToDir ../../public './views/**/*.html.js'",
    "predev": "npm run clean",
    "dev": "cross-env NODE_ENV=development reacthtmlpack buildToDir ../../public './views/**/*.html.js'",
    "prestart": "npm run clean",
    "start": "cross-env NODE_ENV=development reacthtmlpack devServer ./Client.webpackConfig.js ../../public './views/**/*.html.js'"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "reacthtmlpack": "^1.2.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "animate.css": "^3.4.0",
    "bootstrap-sass": "^3.3.6",
    "font-awesome": "^4.5.0",
    "medium-editor": "^5.13.0",
    "medium-editor-tc-mention": "^2.1.1",
    "node-libs-browser": "^1.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-github-fork-ribbon": "^0.4.2",
    "toastr": "^2.1.2"
  }
}
